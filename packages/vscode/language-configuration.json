{
  "comments": {
    "lineComment": "#"
  },
  "brackets": [
    ["(", ")"]
  ],
  "autoClosingPairs": [
    { "open": "(", "close": ")" },
    { "open": "\"", "close": "\"", "notIn": ["string"] }
  ],
  "surroundingPairs": [
    ["(", ")"],
    ["\"", "\""]
  ],
  "indentationRules": {
    "increaseIndentPattern": "^\\s*[a-zA-Z_$][a-zA-Z0-9_$-]*\\s*:$",
    "decreaseIndentPattern": "^\\s*(\\}|\\])"
  },
  "folding": {
    "markers": {
      "start": "^\\s*[a-zA-Z_$][a-zA-Z0-9_$-]*\\s*:$",
      "end": "^\\s*$"
    }
  }
}
