{
  "name": "lean-format-monorepo",
  "version": "1.0.0",
  "description": "LEAN Format - Complete project repository",
  "private": true,
  "workspaces": [
    "packages/core",
    "packages/cli",
    "packages/vscode",
    "packages/playground",
    "packages/website"
