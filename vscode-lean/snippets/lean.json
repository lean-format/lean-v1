{
  "Key-Value Pair": {
    "prefix": "kv",
    "body": [
      "${1:key}: ${2:value}"
    ],
    "description": "Insert a key-value pair"
  },
  "Object": {
    "prefix": "obj",
    "body": [
      "${1:object}:",
      "    ${2:key}: ${3:value}"
    ],
    "description": "Insert an object with nested key-value"
  },
  "Simple List": {
    "prefix": "list",
    "body": [
      "${1:list}:",
      "    - ${2:item}"
    ],
    "description": "Insert a simple list"
  },
  "Row Syntax": {
    "prefix": "rows",
    "body": [
      "${1:name}(${2:col1}, ${3:col2}, ${4:col3}):",
      "    - ${5:val1}, ${6:val2}, ${7:val3}"
    ],
    "description": "Insert row syntax with header tuple"
  },
  "List of Objects": {
    "prefix": "objlist",
    "body": [
      "${1:list}:",
      "    - ${2:key}: ${3:value}",
      "      ${4:key2}: ${5:value2}"
    ],
    "description": "Insert a list of objects"
  },
  "Comment": {
    "prefix": "comment",
    "body": [
      "# ${1:comment}"
    ],
    "description": "Insert a comment"
  },
  "Section Header": {
    "prefix": "section",
    "body": [
      "# ============================================================================",
      "# ${1:Section Name}",
      "# ============================================================================",
      "$0"
    ],
    "description": "Insert a section header comment"
  },
  "User Table": {
    "prefix": "users",
    "body": [
      "users(id, name, email, age, active):",
      "    - ${1:1}, ${2:Alice}, ${3:alice@example.com}, ${4:30}, ${5:true}"
    ],
    "description": "Insert a users table template"
  },
  "Product Catalog": {
    "prefix": "products",
    "body": [
      "products(id, name, price, stock):",
      "    - ${1:1}, ${2:Product Name}, ${3:19.99}, ${4:50}"
    ],
    "description": "Insert a products table template"
  },
  "Config Object": {
    "prefix": "config",
    "body": [
      "config:",
      "    debug: ${1:false}",
      "    timeout: ${2:5000}",
      "    features:",
      "        ${3:api}: ${4:true}",
      "        ${5:cache}: ${6:true}"
    ],
    "description": "Insert a configuration object template"
  }
}
